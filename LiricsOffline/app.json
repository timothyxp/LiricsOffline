{
  "expo": {
    "name": "LiricsOffline",
    "slug": "LiricsOffline",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon_app.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.timothyxp.LiricsOffline",
       "supportsTablet": true
    },
    "android": {
      "package": "com.timothyxp.LiricsOffline"
    }
  }
}